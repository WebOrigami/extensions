// This file defines the site structure, using Handlebars templates to
// generate the index page and team member pages.
{
  index.html = index.hbs(teamData.yaml)
  team = @map(teamData.yaml, {
    keyMap: (person) => `${person/name}.html`
    valueMap: person.hbs
  })
  assets
  images
  thumbnails = @map(images, @image/resize({ width: 200 }))
  test.html = message
}
